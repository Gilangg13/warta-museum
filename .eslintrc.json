{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": "off",
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "comma-dangle": 0,
        "implicit-arrow-linebreak": 0,
        "indent": 0,
        "no-use-before-define": [
            "error",
            {
                "functions": false,
                "classes": false
            }
        ],
        "arrow-body-style": [
            "error",
            "as-needed"
        ],
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "import/prefer-default-export": 0
    }
}